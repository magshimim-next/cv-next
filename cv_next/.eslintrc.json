{
  "extends": ["next/core-web-vitals", "plugin:react-hooks/recommended"],
  "plugins": ["unused-imports"],
  "rules": {
    "@next/next/no-html-link-for-pages": "off",
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ],
    "no-console": "warn"
  }
}
